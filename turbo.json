{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "dependsOn": [
        "^dev"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "outputs": []
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "inputs": [
        "src/**",
        "tests/**",
        "pyproject.toml",
        "requirements.txt"
      ],
      "outputs": []
    },
    "type-check": {
      "dependsOn": [
        "^type-check"
      ],
      "outputs": []
    },
    "format": {
      "outputs": []
    }
  }
}
